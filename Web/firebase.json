{
  "functions": {
      "codebase": "functions"
    },

  "emulators": {
    "redirects": [
     
    ],

    "firestore": {
      "port": 13080
    },
    "ui": {
      "enabled": true,
      "port": 4000
    },
    "auth": {
      "port": 7080
    },
    "functions": {
      "port": 5080
    },
    "database": {
      "port": 6080
    },
    "hosting": {
      "port": 10080
    },
    "pubsub": {
      "port": 11080
    }
  },

  "hosting": {
    "public": "dist/Zuri",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],



    "headers": [
      {
        "source": "https://connect.squareupsandbox.com/*",
        "headers": [
          {
            "key": "Access-Control-Allow-Origin",
            "value": "*"
          }
        ]
      }
    ]
  }
}
